opTypes.func.isRequired\r\n};\r\n\r\nlet mapStateToProps = state => {\r\n  return {\r\n    user: state.user,\r\n    movies: state.movies\r\n  }\r\n}\r\n\r\nexport default connect(mapStateToProps)(DirectorView)"},"lineCount":null}},"error":null,"hash":"d8f40f1236421333eaf823187c63dd4f","cacheData":{"env":{}}}